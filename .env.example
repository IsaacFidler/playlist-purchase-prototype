# Rename to .env.local and fill in values for local development.

# Supabase - Get these from your Supabase project settings
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Database - PostgreSQL connection string from Supabase
DATABASE_URL=postgresql://postgres:[password]@db.your-project.supabase.co:5432/postgres

# Spotify OAuth - Get these from Spotify Developer Dashboard
NEXT_PUBLIC_SPOTIFY_CLIENT_ID=
SPOTIFY_CLIENT_ID=
SPOTIFY_CLIENT_SECRET=
NEXT_PUBLIC_SPOTIFY_REDIRECT_URI=http://localhost:3000/auth/callback
SPOTIFY_AUTH_ENDPOINT=https://accounts.spotify.com/authorize
SPOTIFY_TOKEN_ENDPOINT=https://accounts.spotify.com/api/token

# Vendor APIs
NEXT_PUBLIC_ITUNES_COUNTRY=GB
DISCOGS_TOKEN=
